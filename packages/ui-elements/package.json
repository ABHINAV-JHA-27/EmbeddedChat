{
    "name": "@embeddedchat/ui-elements",
    "version": "0.0.0",
    "description": "Reusable UI components and UI-related hooks for EmbeddedChat.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/RocketChat/EmbeddedChat"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "rollup -c --context=window --environment NODE_ENV:production",
        "watch": "rollup -c --watch --context=window",
        "dev": "concurrently \" npm run watch \" \" npm run start --prefix playground \"",
        "test": "run-s test:unit test:lint test:build",
        "test:lint": "eslint src/**/*.js",
        "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
        "test:watch": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.js",
        "test:build": "run-s build",
        "format": "prettier --write 'src/' ",
        "format:check": "prettier --check 'src/' ",
        "lint": "eslint 'src/**/*.js'",
        "lint:fix": "eslint 'src/**/*.js' --fix",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "build-icons": "node tools/icons-generator.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.7",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^5.0.2",
        "concurrently": "^7.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.30.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.12",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-bundle-size": "^1.0.3",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.66.1",
        "storybook": "^7.0.26"
    },
    "peerDependencies": {
        "react": ">=17.0.2 <19.0.0",
        "react-dom": ">=17.0.2 <19.0.0"
    },
    "dependencies": {
        "@babel/eslint-parser": "^7.24.7",
        "@emotion/babel-preset-css-prop": "^11.11.0",
        "@emotion/react": "11.7.1",
        "@rollup/plugin-json": "^6.0.0",
        "color": "^4.2.3",
        "normalize.css": "^8.0.1",
        "prop-types": "^15.8.1"
    }
}
